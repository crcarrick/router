{
  "name": "router",
  "private": true,
  "version": "1.0.0",
  "description": "React router clone",
  "main": "index.js",
  "scripts": {
    "build": "pnpm run -r build",
    "clean": "pnpm run -r clean",
    "tests": "pnpm run -r tests",
    "watch": "pnpm run -r --parallel watch",
    "fresh": "find . -name 'node_modules' -type d -prune -exec echo 'Deleting {}' \\; -exec rm -rf {} \\; && pnpm install",
    "prepare": "husky",
    "precommit": "lint-staged",
    "generate:example": "pnpm run --filter @router/generator generate:example",
    "generate:package": "pnpm run --filter @router/generator generate:package",
    "examples:start": "pnpm run --filter $NAME start",
    "examples:start:01_basic": "NAME=01_basic pnpm run examples:start",
    "examples:start:02_loaders": "NAME=02_loaders pnpm run examples:start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@crcarrick/eslint-config": "^1.0.0",
    "@crcarrick/tsconfig": "^1.0.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "handlebars": "^4.7.8",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "typescript": "5.4.0-beta"
  }
}
