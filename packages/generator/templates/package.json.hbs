{
  "name": "{{{name}}}",
  "version": "1.0.0",
  {{#if private}}
  "private": true,
  {{/if}}
  "description": "",
  "author": {
    "name": "Chris Carrick",
    "email": "chris@crcarrick.dev"
  },
  "main": "dist/index.js",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    {{#each scripts}}
    "{{{name}}}": "{{{command}}}"{{#unless @last}},{{/unless}}
    {{/each}}
  },
  {{#each dependencies}}
  "{{@key}}": {
    {{#each this}}
    "{{{name}}}": "{{{version}}}"{{#unless @last}},{{/unless}}
    {{/each}}
  }{{#unless @last}},{{/unless}}
  {{/each}}
}