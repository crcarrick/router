import {
  createBrowserRouter,
  Outlet,
  RouterProvider,
} from '@crcarrick/router'

function Layout() {
  return (
    <div>
      <h1>{{{name}}}</h1>
      <Outlet />
    </div>
  )
}

const router = createBrowserRouter([
  {
    path: '/',
    element: <Layout />, 
  }
])

export default function App() {
  return <RouterProvider router={router} />
}